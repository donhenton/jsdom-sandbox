{
  "name": "jsdom-sandbox",
  "scripts": {
    "clean": "del-cli coverage",
    "test": "mocha test",
    "test:debug": "LOG_LEVEL=debug npm test -- debug",
    "test:watch": "npm test -- --watch",
    "test:unit": "npm test -- -g ^Unit::",
    "test:int": "npm test -- -g ^Integration::",
    "test:coverclean": "npm run clean && npm run test:cover",
    "test:cover": "istanbul cover ./node_modules/.bin/_mocha --  --report html  --recursive test  || exit 0"
  },
  "version": "1.0.0",
  "keywords": [
    "util",
    "functional",
    "server",
    "client",
    "browser"
  ],
  "author": "donhenton@gmail.com",
  "contributors": [],
  "devDependencies": {
    "backbone": "^1.3.3",
    "chai": "^3.5.0",
    "del-cli": "^1.1.0",
    "fs": "0.0.2",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "jsonpath": "0.2.11",
    "mocha": "^3.2.0",
    "mocha-jsdom": "^1.1.0",
    "rootpath": "^0.1.2",
    "sinon": "^1.17.6",
    "underscore": "^1.8.3",
    "xml2js": "^0.4.19"
  },
  "dependencies": {}
}
